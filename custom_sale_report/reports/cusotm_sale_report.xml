        <field name="name">Custom Sale Report</field>
        <field name="model">custom.sale.report</field>
        <field name="report_type">xlsx</field>
        <field name="report_name">custom_sale_report.action_custom_sale_report_wizard</field>
        <field name="report_file">custom_sale_report.report.action_custom_sale_report_wizard</field>
        <field name="binding_type">report</field>
