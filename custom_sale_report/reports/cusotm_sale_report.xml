    <record id="action_report_pdf" model="ir.actions.report">
        <field name="name">Custom Sale Report PDF</field>
        <field name="model">custom.sale.report</field>
        <field name="report_type">qweb-pdf</field>
        <field name="report_name">custom_sale_report.report_action_report_pdf</field>
        <field name="report_file">custom_sale_report.report_action_report_pdf</field>
        <field name="binding_model_id" ref="model_custom_sale_report"/>
